{
  "name": "pizzeria-vitepress",
  "version": "0.0.0",
  "homepage": "https://pizzeria-vitepress.netlify.app/",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinegrow/pizzeria-vitepress.git"
  },
  "scripts": {
    "dev": "vitepress dev --host",
    "now": "npm run build && npm run preview",
    "lighthouse": "npx unlighthouse --no-cache --site https://pizzeria-vitepress.netlify.app",
    "build": "vitepress build",
    "preview": "vitepress serve",
    "typecheck": "vue-tsc --noEmit",
    "vue-devtools": "vue-devtools",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint --fix --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts ./ --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts,html,css,md,mdx,json}\" !public --ignore-path .gitignore"
  },
  "engines": {
    "node": "^14.18 || >= 16.0.0"
  },
  "dependencies": {
    "@vueuse/core": "^10.0.2",
    "@vueuse/head": "^1.1.26",
    "defu": "^6.1.2",
    "lite-youtube-embed": "^0.2.0",
    "pinia": "^2.1.6",
    "vee-validate": "^4.11.6",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.51",
    "@nuxt/eslint-config": "^0.1.1",
    "@pinegrow/tailwindcss-plugin": "^3.0.35",
    "@pinegrow/vite-plugin": "^3.0.68",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^20.5.0",
    "@unocss/preset-icons": "^0.57.2",
    "@vue/devtools": "^6.5.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "npm-run-all": "^4.1.5",
    "ofetch": "^1.0.1",
    "postcss": "^8.4.22",
    "prettier": "^2.8.4",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0",
    "unocss": "^0.57.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite-plugin-vue-devtools": "^7.3.6",
    "vitepress": "^1.0.0-rc.4",
    "vue-tsc": "^1.2"
  },
  "web-types": "web-types.json",
  "packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1"
